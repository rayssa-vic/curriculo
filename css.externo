body {
  font-family: Arial, sans-serif;
  margin: 40px;
}

header {
  text-align: center;
  margin-bottom: 30px;
}

header h1 {
  font-size: 28px;
  margin: 0;
}

header h2 {
  font-size: 18px;
  font-weight: normal;
  color: gray;
  margin: 5px 0 0;
}

section {
  margin-bottom: 20px;
}

h3 {
  border-bottom: 1px solid #000;
  padding-bottom: 5px;
}
